FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedFluxPressure;//zeroGradient;
        value           uniform 0;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
        //type            totalPressure;
        //p0              uniform 0;
    }

    top
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    bottom
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    "array_cavity_bottom_.*"
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    "array_cavity_wall_.*"
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    front
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    back
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    symmetryPlane
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
}


